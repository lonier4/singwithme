<br />
  <div class="booyah-box col-8 offset-2">
    <div class="text center">
      <video id="my-video" class="video-js vjs-big-play-centered" controls preload="auto" width="990" height="700"
      poster="<%= image_path('black.jpg') %>" data-setup="{}">
        <source src="<%= current_user_video.video.to_s %>" type='video/mp4'>
        <source src="<%= current_user_video.video.to_s %>" type='video/webm'>
          <noscript>
            <p class="vjs-no-js">
              To view this video please enable JavaScript, and consider upgrading to a web browser that
              <a href="http://videojs.com/html5-video-support/" target="_blank">supports HTML5 video</a>
            </p>
          </noscript>
          </video>
          </br>
          <h4>
            <%= current_user_video.title %> 
            -
            <%= current_user_video.artist %>
          </h4> 
        </div>
      </div>
      </br>
    </div>
  </div>
</div>

  

