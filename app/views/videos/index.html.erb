<br />

<div class="booyah-box col-10 offset-1">
  <h1 class="text-center">Checkout Some Performances</h1>
  <hr />

  <% if @videos_from_same_artist != nil %>
    <% @videos_from_same_artist.each do |video| %>
      <h2 class="text-center"><%= video.title %></h2>
      <div class="col-sm-8 col-12">
        <%= video.artist %>
      </div>
    <% end %>
  <% end %>
</div>