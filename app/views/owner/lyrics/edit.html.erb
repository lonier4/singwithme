<br />
<div class="booyah-box col-10 offset-1">
  <h1>Edit Your Videos</h1>
    <%= simple_form_for @lyric, url: owner_lyric_path, html:  {method: :put} do |f| %>
      <%= f.input :title %>
      <%= f.input :artist %>
      <%= f.input :video_url %>
      <br />
      <%= f.submit "Update", class: 'btn btn-primary' %>
      <br />
    <% end %>
  <br />
</div>